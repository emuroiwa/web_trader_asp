    <script>
  
      
        //check if master is on pop up
        if(window.opener != null)
        {            ////PopUp
            //window.close();
        }
        else        {
            //Not a Popup
            window.location = "notpopup.aspx";        }

        //alert("hhhh")
   
        function GetUrlFolder() {
            var url = window.location.pathname;
            var dir = url.substring(0, url.lastIndexOf('/'));
            return dir;
        }
        function treeview(urlfolder) {
            if (urlfolder == GetUrlFolder()) {
                var cssclass = '<li class="active treeview">';
            } else {
                var cssclass = '<li class="treeview">';
            }
            return cssclass
        }
        if (!GetUrlFolder()) {
            document.title = "TDS | Treasury Dealing System Home";
        }
        else {
            document.title = "TDS | " + GetUrlFolder().substring(1);

        }
    </script>
