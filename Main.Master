    <!-- timer -->
    
     <script src="../../dist/js/jquery.min.js"></script>
    <script>
        $(function () {
            $("body").on('click keypress', function () {
                ResetThisSession();
                audio.pause();

            });
        });
        document.addEventListener("mouseover", myFunction);
        function myFunction() {
            ResetThisSession();
            audio.pause();
        }
        //audio
        if (!GetUrlFolder()) {
            var audio = new Audio('ee.mp3');
        } else {
            var audio = new Audio('../ee.mp3');
        }
        var timeInSecondsAfterSessionOut = document.getElementById("<%=lblSessionTime.ClientID%>").innerHTML * 60;

        var secondTick = 0;

        function ResetThisSession() {
            secondTick = 0;
        }

        function StartThisSessionTimer() {
            secondTick++;
            var timeLeft = ((timeInSecondsAfterSessionOut - secondTick) / 60).toFixed(0); // in minutes
            timeLeft = timeInSecondsAfterSessionOut - secondTick;
            // warn log off
            if (timeLeft == 10) {
                audio.play();
            }
            minutes = (timeLeft / 60) | 0;
            seconds = (timeLeft % 60) | 0;

            minutes = minutes < 10 ? "0" + minutes : minutes;
            seconds = seconds < 10 ? "0" + seconds : seconds;

            $("#lblSessionTime").html(minutes + ":" + seconds);

            if (secondTick >= timeInSecondsAfterSessionOut) {
                clearTimeout(tick);
                if (!GetUrlFolder()) {
                    window.close()
                    window.opener.location.href = "lockscreen.aspx";
                    window.onbeforeunload = RefreshParent;


                } else {
                    window.close()
                    window.opener.location.href = "../lockscreen.aspx";
                    window.onbeforeunload = RefreshParent;


                }
                return;
            }
            tick = setTimeout("StartThisSessionTimer()", 1000);
        }

        StartThisSessionTimer();
</script>
